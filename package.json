{
	"name": "portfolio",
	"description": "Portfolio Site",
	"version": "0.0.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/alexaegis"
	},
	"homepage": "https://www.github.com/alexaegis/alexaegis#readme",
	"repository": "github:alexaegis/alexaegis",
	"bugs": {
		"url": "https://www.github.com/alexaegis/alexaegis/issues"
	},
	"keywords": [
		"ng",
		"angular",
		"portfolio",
		"alexaegis",
		"nx",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"sideEffects": [
		"./apps/*/src/main.ts"
	],
	"private": true,
	"config": {
		"engine-strict": true
	},
	"engines": {
		"node": ">=14.0.0",
		"npm": ">=7.0.0"
	},
	"workspaces": [
		"apps/*",
		"libs/*"
	],
	"scripts": {
		"ng": "nx",
		"nx": "nx",
		"start": "ng serve",
		"start:portfolio": "ng serve portfolio",
		"build": "nx affected:build --all --prod",
		"build:portfolio": "nx build portfolio --prod --stats-json",
		"test": "ng test",
		"test:skip-cache": "nx run-many --all --target test --skip-nx-cache",
		"lint": "nx workspace-lint && ng lint",
		"lint:css": "stylelint --allow-empty-input --syntax=css ./**/*.css",
		"lint:scss": "stylelint --allow-empty-input --syntax=scss ./**/*.scss",
		"lint:md": "markdownlint --ignore 'CHANGELOG.md' --ignore-path '.gitignore' .",
		"lint:rmd": "remark .",
		"lint:format": "prettier --check .",
		"e2e": "ng e2e",
		"affected:apps": "nx affected:apps",
		"affected:libs": "nx affected:libs",
		"affected:build": "nx affected:build",
		"affected:e2e": "nx affected:e2e",
		"affected:test": "nx affected:test",
		"affected:lint": "nx affected:lint",
		"affected:dep-graph": "nx affected:dep-graph",
		"affected": "nx affected",
		"format": "nx format:write",
		"format:write": "nx format:write",
		"format:check": "nx format:check",
		"ncu": "ncu -u",
		"update": "nx migrate latest",
		"analyze": "webpack-bundle-analyzer dist/apps/portfolio/stats.json",
		"workspace-generator": "nx workspace-generator",
		"dep-graph": "nx dep-graph",
		"help": "nx help",
		"postinstall": "husky install && node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
		"prepublishOnly": "pinst --disable && npm run build",
		"postpublish": "pinst --enable",
		"scully": "npx scully --scanRoutes --showGuessError --",
		"scully:serve": "npx scully serve --"
	},
	"dependencies": {
		"@angular/animations": "11.2.12",
		"@angular/common": "11.2.12",
		"@angular/compiler": "11.2.12",
		"@angular/core": "11.2.12",
		"@angular/forms": "11.2.12",
		"@angular/platform-browser": "11.2.12",
		"@angular/platform-browser-dynamic": "11.2.12",
		"@angular/router": "11.2.12",
		"@nrwl/angular": "12.1.0",
		"@scullyio/ng-lib": "^1.1.1",
		"@scullyio/scully": "^1.1.1",
		"rxjs": "~6.5.3",
		"tslib": "^2.2.0",
		"zone.js": "^0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "0.1102.11",
		"@angular-eslint/eslint-plugin": "4.2.0",
		"@angular-eslint/eslint-plugin-template": "4.2.0",
		"@angular-eslint/template-parser": "4.2.0",
		"@angular/cli": "11.2.11",
		"@angular/compiler-cli": "11.2.12",
		"@angular/language-service": "11.2.12",
		"@commitlint/cli": "^12.1.1",
		"@commitlint/config-conventional": "^12.1.1",
		"@nrwl/cli": "12.1.0",
		"@nrwl/cypress": "12.1.0",
		"@nrwl/eslint-plugin-nx": "12.1.0",
		"@nrwl/jest": "12.1.0",
		"@nrwl/linter": "12.1.0",
		"@nrwl/tao": "12.1.0",
		"@nrwl/workspace": "12.1.0",
		"@scullyio/init": "^1.1.4",
		"@types/jest": "26.0.23",
		"@types/node": "15.0.1",
		"@typescript-eslint/eslint-plugin": "4.22.0",
		"@typescript-eslint/parser": "4.22.0",
		"cypress": "^6.9.1",
		"dotenv": "8.2.0",
		"eslint": "7.25.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-cypress": "^2.11.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsdoc": "^33.0.0",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "~3.4.0",
		"husky": "^6.0.0",
		"jest": "26.6.3",
		"jest-preset-angular": "8.4.0",
		"lint-staged": "^10.5.4",
		"markdownlint": "^0.23.1",
		"markdownlint-cli": "^0.27.1",
		"npm-check-updates": "~11.5.10",
		"pinst": "^2.1.6",
		"prettier": "2.2.1",
		"remark": "^13.0.0",
		"remark-cli": "^9.0.0",
		"remark-lint": "^8.0.0",
		"remark-lint-maximum-line-length": "^2.0.3",
		"remark-preset-lint-consistent": "^4.0.0",
		"remark-preset-lint-recommended": "^5.0.0",
		"rimraf": "^3.0.2",
		"standard-version": "^9.2.0",
		"stylelint": "^13.13.1",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-prettier": "^1.2.0",
		"ts-jest": "26.5.5",
		"ts-node": "~9.1.1",
		"typescript": "4.1.4",
		"webpack-bundle-analyzer": "~4.4.1"
	}
}
